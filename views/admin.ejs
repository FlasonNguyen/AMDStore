<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>KStore - AMD's Collaborator</title>
    <link rel="icon" type="image/x-icon" href="/images/logo_red.jpg" />

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Titillium+Web:400,200,300,700,600' rel='stylesheet'
        type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,300' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,100' rel='stylesheet' type='text/css'>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="/stylesheet/owl.carousel.css">
    <link rel="stylesheet" href="/stylesheet/style.css">
    <link rel="stylesheet" href="/stylesheet/responsive.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <div class="header-area">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="user-menu">
                        <% if(!locals.user) { %>
                            <ul>
                                <li><a href="#" data-toggle="modal" data-target="#SignUpModal"><i
                                            class="fa fa-user"></i>
                                        Sign Up</a></li>
                                <li><a href="#" data-toggle="modal" data-target="#SignInModal"><i
                                            class="fa fa-user"></i>
                                        Sign In</a></li>
                            </ul>
                            <% } else { %>
                                <ul>
                                    <li><a href="/account/signout"><i class="fa fa-user"></i>
                                            Sign Out</a></li>
                                </ul>
                                <% } %>
                    </div>
                </div>
            </div>
            <div class="modal fade" id="SignUpModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <form action="/account/signup" method="post">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type=" button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                                <h5 class="modal-title" id="exampleModalLabel">
                                    <h3 class="text-center"">Sign Up</h3>
                        </h5>
                        
                    </div>
                    <div class=" modal-body" style="margin: auto; width: 50%; padding: 10px">

                                        <span>Email:</span><br>
                                        <input style="width: 100%;" type="email" name="email" required><br>
                                        <span>Password:</span><br>
                                        <input style="width: 100%;" type="password" name="password" required><br>
                                        <span>Phone:</span><br>
                                        <input style="width: 100%;" type="text" name="phone" required><br>
                                        <span>Address:</span><br>
                                        <input style="width: 100%;" type="text" name="address" required><br>
                                        <span>Name:</span><br>
                                        <input style="width: 100%;" type="text" name="name" required><br>

                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="submit" class="btn btn-primary">Save changes</button>
                            </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="modal fade" id="SignInModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog" role="document">
                <form action="/account/signin" method="post">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type=" button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h5 class="modal-title" id="exampleModalLabel">
                                <h3 class="text-center"">Login</h3>
                        </h5>
                        
                    </div>
                    <div class=" modal-body" style="margin: auto; width: 50%; padding: 10px">

                                    <span>Email:</span><br>
                                    <input style="width: 100%;" type="email" name="email" id="emailinp" required><br>
                                    <span>Password:</span><br>
                                    <input style="width: 100%;" type="password" name="password" id="passwordinp"
                                        required><br>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                </form>
            </div>
        </div>

    </div>
    </div> <!-- End header area -->

    <div class="site-branding-area">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <div class="logo">
                        <h1><a href="/"><span>AMD</span> - KStore</a></h1>
                    </div>
                </div>
                <% if (locals.user) { %>
                    <div class="col-sm-6">
                        <div class="shopping-item">
                            <a href="#">User: <span class="cart-amunt">
                                    <%= locals.user %>
                                </span></a>
                        </div>
                    </div>
                    <% } %>
            </div>
        </div>
    </div> <!-- End site branding area -->

    <div class="mainmenu-area">
        <div class="container">
            <div class="row">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="/">Home</a></li>
                        <li class="active"><a href="/kstore">Processors</a></li>
                        <li class="active"><a href="#" style="
                            cursor: not-allowed;">Features ( Coming Soon )</a></li>
                        <li>
                            <form action="/kstore/search" method="get"><input
                                    style="margin-left: 200px; margin-top: 10px" type="text" placeholder="Search.."
                                    name="product"><button type="submit" style="border: 1px solid #ccc">Search</button>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div> <!-- End mainmenu area -->

    <div class="maincontent-area">
        <div class="zigzag-bottom"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <a href="" type="button" class="btn btn-primary admin" data-toggle="modal"
                        data-target="#AddProduct"><span>Add Product</span></a>
                    <a href="" type="button" class="btn btn-danger admin" data-toggle="modal"
                        data-target="#DeleteProduct"><span>Delete Product</span></a>
                    <a href="" type="button" class="btn btn-primary admin" data-toggle="modal"
                        data-target="#UpdateProduct"><span>Update Product</span></a>
                    <a href="/kstore" type="button" class="btn btn-primary admin"><span>Product List</span></a>
                    <a href="" type="button" class="btn btn-primary admin" disabled><span>User List (Coming
                            Soon)</span></a>
                    <a href="/account/signup" type="button" class="btn btn-primary admin" disabled><span>Create Admin
                            (Coming Soon)<br>
                            Account</span></a>
                    <a href="#" type="button" class="btn btn-primary admin" disabled><span>Support ( Coming Soon
                            )</span></a>
                </div>
                <div class="modal fade" id="AddProduct" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                    aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <form action="/kstore/model/add" method="post">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type=" button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                    <h5 class="modal-title" id="exampleModalLabel">
                                        <h3 class="text-center"">Add Product</h3>
                        </h5>
                        
                    </div>
                    <div class=" modal-body" style="margin: auto; width: 50%; padding: 10px">

                                            <span>Model:</span><br>
                                            <input style="width: 100%;" type="text" name="model" required><br>
                                            <span style="text-align: center">Specs</span>
                                            <span>Platform:</span><br>
                                            <input style="width: 100%;" type="text" name="platform" required><br>
                                            <span>ProductFamily:</span><br>
                                            <input style="width: 100%;" type="text" name="productFamily" required><br>
                                            <span>CPU Cores:</span><br>
                                            <input style="width: 100%;" type="text" name="cpuCore" required><br>
                                            <span>CPU Thread:</span><br>
                                            <input style="width: 100%;" type="text" name="cpuThread" required><br>
                                            <span>CPU Core Technology:</span><br>
                                            <input style="width: 100%;" type="text" name="cpuCoreTech" required><br>
                                            <span>CPU Base Clock:</span><br>
                                            <input style="width: 100%;" type="text" name="baseClock" required><br>
                                            <span>CPU Max Clock:</span><br>
                                            <input style="width: 100%;" type="text" name="maxClock" required><br>
                                            <span>CPU Max Temperature:</span><br>
                                            <input style="width: 100%;" type="text" name="maxTemp" required><br>
                                            <span>CPU Architecture:</span><br>
                                            <input style="width: 100%;" type="text" name="architecture" required><br>
                                            <span>CPU Support Technology:</span><br>
                                            <input style="width: 100%;" type="text" name="supportTech" required><br>
                                            <span style="text-align: center">Connectivity</span>
                                            <span>PCI Version:</span><br>
                                            <input style="width: 100%;" type="text" name="cpiVer" required><br>
                                            <span>CPU Memory Type:</span><br>
                                            <input style="width: 100%;" type="text" name="memoryType" required><br>
                                            <span>CPU Memory Channels:</span><br>
                                            <input style="width: 100%;" type="text" name="memoryChannels" required><br>
                                            <span>CPU Memory Spec:</span><br>
                                            <input style="width: 100%;" type="text" name="memorySpec" required><br>
                                            <span>Release Date</span><br>
                                            <input style="width: 100%;" type="date" name="releaseDate" required><br>
                                            <span>Price:</span><br>
                                            <input style="width: 100%;" type="text" name="price" required><br>
                                            <span>CPU Series:</span><br>
                                            <input style="width: 100%;" type="text" name="series" required><br>
                                            <span>CPU Picture URL:</span><br>
                                            <input style="width: 100%;" type="url" name="picture" required><br>

                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    <button type="submit" class="btn btn-primary">Save changes</button>
                                </div>
                        </form>
                    </div>
                </div>

            </div>
            <div class="modal fade" id="DeleteProduct" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <form action="/kstore/model/remove" method="post">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type=" button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                                <h5 class="modal-title" id="exampleModalLabel">
                                    <h3 class="text-center"">Delete Product</h3>
                        </h5>
                        
                    </div>
                    <div class=" modal-body" style="margin: auto; width: 50%; padding: 10px">

                                        <span>Product ID:</span><br>
                                        <input style="width: 100%;" type="text" name="id" required><br>


                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="submit" class="btn btn-primary">Save changes</button>
                            </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="modal fade" id="UpdateProduct" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog" role="document">
                <form action="/kstore/model/update" method="post">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type=" button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h5 class="modal-title" id="exampleModalLabel">
                                <h3 class="text-center"">Update Product</h3>
                        </h5>
                        
                    </div>
                    <div class=" modal-body" style="margin: auto; width: 50%; padding: 10px">

                                    <span><strong>Product ID:</strong></span><br>
                                    <input style="width: 100%;" type="text" name="id" required><br>
                                    <span>Model:</span><br>
                                    <input style="width: 100%;" type="text" name="model" required><br>
                                    <span style="text-align: center">Specs</span>
                                    <span>Platform:</span><br>
                                    <input style="width: 100%;" type="text" name="platform" required><br>
                                    <span>ProductFamily:</span><br>
                                    <input style="width: 100%;" type="text" name="productFamily" required><br>
                                    <span>CPU Cores:</span><br>
                                    <input style="width: 100%;" type="text" name="cpuCore" required><br>
                                    <span>CPU Thread:</span><br>
                                    <input style="width: 100%;" type="text" name="cpuThread" required><br>
                                    <span>CPU Core Technology:</span><br>
                                    <input style="width: 100%;" type="text" name="cpuCoreTech" required><br>
                                    <span>CPU Base Clock:</span><br>
                                    <input style="width: 100%;" type="text" name="baseClock" required><br>
                                    <span>CPU Max Clock:</span><br>
                                    <input style="width: 100%;" type="text" name="maxClock" required><br>
                                    <span>CPU Max Temperature:</span><br>
                                    <input style="width: 100%;" type="text" name="maxTemp" required><br>
                                    <span>CPU Architecture:</span><br>
                                    <input style="width: 100%;" type="text" name="architecture" required><br>
                                    <span>CPU Support Technology:</span><br>
                                    <input style="width: 100%;" type="text" name="supportTech" required><br>
                                    <span style="text-align: center">Connectivity</span>
                                    <span>PCI Version:</span><br>
                                    <input style="width: 100%;" type="text" name="cpiVer" required><br>
                                    <span>CPU Memory Type:</span><br>
                                    <input style="width: 100%;" type="text" name="memoryType" required><br>
                                    <span>CPU Memory Channels:</span><br>
                                    <input style="width: 100%;" type="text" name="memoryChannels" required><br>
                                    <span>CPU Memory Spec:</span><br>
                                    <input style="width: 100%;" type="text" name="memorySpec" required><br>
                                    <span>Release Date</span><br>
                                    <input style="width: 100%;" type="date" name="releaseDate" required><br>
                                    <span>Price:</span><br>
                                    <input style="width: 100%;" type="text" name="price" required><br>
                                    <span>CPU Series:</span><br>
                                    <input style="width: 100%;" type="text" name="series" required><br>
                                    <span>CPU Picture URL:</span><br>
                                    <input style="width: 100%;" type="url" name="picture" required><br>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                </form>
            </div>
        </div>
    </div> <!-- End main content area -->

    <div class="brands-area">
        <div class="zigzag-bottom"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="brand-wrapper">
                        <h2 class="section-title">Brands</h2>
                        <div class="brand-list">
                            <img src="https://phongvu.vn/cong-nghe/wp-content/uploads/2018/04/asus_logo_ISOI-1024x536.png"
                                alt="">
                            <img src="https://www.amd.com/system/files/styles/992px/private/2021-07/921575-amd-microsoft-windows-glow-1260x709.png?itok=Ewmrhxda"
                                alt="">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End brands area -->

    <div class="product-widget-area">
        <div class="zigzag-bottom"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="single-product-widget">
                        <h2 class="product-wid-title">Top Sellers</h2>
                        <a href="" class="wid-view-more">View All</a>
                        <% if (products) { %>
                            <% for(let i=0; i < 3; i++) { %>
                                <div class="single-wid-product">
                                    <a href="/kstore/processors/<%= products[i]._id %>"><img
                                            src="<%= products[i].picture %>" alt="" class="product-thumb"></a>
                                    <h2><a href="/kstore/processors/<%= products[i]._id %>">
                                            <%= products[i].model %>
                                        </a></h2>
                                    <div class="product-wid-rating">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                    </div>
                                    <div class="product-wid-price">
                                        <ins>
                                            <%= products[i].price %>
                                        </ins>
                                    </div>
                                </div>
                                <% } %>
                                    <% } %>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="single-product-widget">
                        <h2 class="product-wid-title">Recently Viewed</h2>
                        <a href="#" class="wid-view-more">View All</a>
                        <% if (products) { %>
                            <% for(let i=0; i < 3; i++) { %>
                                <div class="single-wid-product">
                                    <a href="/kstore/processors/<%= products[i]._id %>"><img
                                            src="<%= products[i].picture %>" alt="" class="product-thumb"></a>
                                    <h2><a href="/kstore/processors/<%= products[i]._id %>">
                                            <%= products[i].model %>
                                        </a></h2>
                                    <div class="product-wid-rating">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                    </div>
                                    <div class="product-wid-price">
                                        <ins>
                                            <%= products[i].price %>
                                        </ins>
                                    </div>
                                </div>
                                <% } %>
                                    <% } %>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="single-product-widget">
                        <h2 class="product-wid-title">Top New</h2>
                        <a href="#" class="wid-view-more">View All</a>
                        <% if (products) { %>
                            <% for(let i=0; i < 3; i++) { %>
                                <div class="single-wid-product">
                                    <a href="/kstore/processors/<%= products[i]._id %>"><img
                                            src="<%= products[i].picture %>" alt="" class="product-thumb"></a>
                                    <h2><a href="/kstore/processors/<%= products[i]._id %>">
                                            <%= products[i].model %>
                                        </a></h2>
                                    <div class="product-wid-rating">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                    </div>
                                    <div class="product-wid-price">
                                        <ins>
                                            <%= products[i].price %>
                                        </ins>
                                    </div>
                                </div>
                                <% } %>
                                    <% } %>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End product widget area -->

    <div class="footer-top-area">
        <div class="zigzag-bottom"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6">
                    <div class="footer-about-us">
                        <h2><span>AMD</span> - KStore</h2>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Perferendis sunt id doloribus vero
                            quam laborum quas alias dolores blanditiis iusto consequatur, modi aliquid eveniet eligendi
                            iure eaque ipsam iste, pariatur omnis sint! Suscipit, debitis, quisquam. Laborum commodi
                            veritatis magni at?</p>
                        <div class="footer-social">
                            <a href="#" target="_blank"><i class="fa fa-facebook"></i></a>
                            <a href="#" target="_blank"><i class="fa fa-twitter"></i></a>
                            <a href="#" target="_blank"><i class="fa fa-youtube"></i></a>
                            <a href="#" target="_blank"><i class="fa fa-linkedin"></i></a>
                            <a href="#" target="_blank"><i class="fa fa-pinterest"></i></a>
                        </div>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6">
                    <div class="footer-menu">
                        <h2 class="footer-wid-title">User Navigation </h2>
                        <ul>
                            <li><a href="#">My account</a></li>
                            <li><a href="#">Vendor contact</a></li>
                            <li><a href="#">Front page</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6">
                    <div class="footer-menu">
                        <h2 class="footer-wid-title">Categories</h2>
                        <ul>
                            <li><a href="#">Desktop</a></li>
                            <li><a href="#">Workstation</a></li>
                            <li><a href="#">Mobile</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6">
                    <div class="footer-newsletter">
                        <h2 class="footer-wid-title">Newsletter</h2>
                        <p>Sign up to our newsletter and get exclusive deals you wont find anywhere else straight to
                            your inbox!</p>
                        <div class="newsletter-form">
                            <form action="#">
                                <input type="email" placeholder="Type your email">
                                <input type="submit" value="Subscribe">
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End footer top area -->

    <div class="footer-bottom-area">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="copyright">
                        <p>Tran Dang Khoa đã ở đây</p>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="footer-card-icon">
                        <i class="fa fa-cc-discover"></i>
                        <i class="fa fa-cc-mastercard"></i>
                        <i class="fa fa-cc-paypal"></i>
                        <i class="fa fa-cc-visa"></i>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End footer bottom area -->

    <!-- Latest jQuery form server -->
    <script src="https://code.jquery.com/jquery.min.js"></script>

    <!-- Bootstrap JS form CDN -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>

    <!-- jQuery sticky menu -->
    <script src="/javascripts/owl.carousel.min.js"></script>
    <script src="/javascripts/jquery.sticky.js"></script>

    <!-- jQuery easing -->
    <script src="/javascripts/jquery.easing.1.3.min.js"></script>

    <!-- Main Script -->
    <script src="/javascripts/main.js"></script>
</body>

</html>